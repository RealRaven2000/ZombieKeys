{
  "author": "Axel Grude",
  "manifest_version": 2,
  "name": "Zombie Keys",
	"description": "Bring dead keys to life: accented letters made easy!",
  "version": "3.0pre9",
  "developer": {
	  "name": "Axel Grude",
    "url": "http://zombiekeys.mozdev.org/index.html"
	},
  "applications": {
    "gecko": {
      "id": "zombiekeys@bolay.de",
      "strict_min_version": "78.0",
      "strict_max_version": "102.*"
    }
  },
  "icons": {
    "32": "chrome/content/zombiekeys.png",
    "64": "chrome/content/zombiekeys64.png"
  },	
  "background": {
    "scripts": ["zk-background.js"]
  },
  "permissions": [
    "compose"
  ],
  "experiment_apis": {
    "LegacyPrefs": {
      "schema": "chrome/content/api/LegacyPrefs/schema.json",
      "parent": {
        "scopes": ["addon_parent"],
        "paths": [["LegacyPrefs"]],
        "script": "chrome/content/api/LegacyPrefs/implementation.js"
      }
    }
  }
}
